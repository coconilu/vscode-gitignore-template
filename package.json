{
  "name": "vscode-gitignore-template",
  "displayName": "vscode-gitignore-template",
  "description": "vscode gitignore template",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.46.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:vscode-gitignore-template.ignore-template"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "vscode-gitignore-template.ignore-template",
        "title": "ignore-template"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run package",
    "compile": "webpack --config ./build/node-extension.webpack.config.js",
    "watch": "webpack --watch --config ./build/node-extension.webpack.config.js",
    "package": "webpack --mode production --devtool hidden-source-map --config ./build/node-extension.webpack.config.js",
    "test-compile": "tsc -p ./",
    "test-watch": "tsc -watch -p ./",
    "pretest": "yarn run test-compile && yarn run lint",
    "lint": "lint-staged",
    "test": "node ./out/test/runTest.js",
    "prepare": "husky install",
    "commit": "check-node-version --node 16 && cz",
    "change-log": "conventional-changelog -p angular -i CHANGELOG.md -s"
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ],
    "*.js": [
      "prettier --write",
      "eslint --cache --fix",
      "git add"
    ],
    "*.vue": [
      "prettier --write",
      "eslint --cache --fix",
      "git add"
    ],
    "*.{json,md,yml,css}": [
      "prettier --write",
      "git add"
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "^16.0.2",
    "@commitlint/config-conventional": "^16.0.0",
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.4",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.63.1",
    "@typescript-eslint/eslint-plugin": "^5.9.1",
    "@typescript-eslint/parser": "^5.9.1",
    "check-node-version": "^4.2.1",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "^8.6.0",
    "eslint-plugin-import": "^2.25.4",
    "glob": "^7.1.6",
    "husky": "^7.0.4",
    "lint-staged": "^12.1.7",
    "mocha": "^8.1.3",
    "prettier": "^2.5.1",
    "ts-loader": "^8.0.11",
    "typescript": "^4.1.2",
    "vscode-test": "^1.4.1",
    "webpack": "^5.10.0",
    "webpack-cli": "^4.2.0"
  },
  "dependencies": {
    "axios": "^0.24.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
